{
  "name": "web-chiaki",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:all": "concurrently \"npm install\" \"cd client && npm install\"",
    "start:all": "concurrently \"npm run start\" \"cd client && npm run serve\"",
    "compile:all": "concurrently \"npm run compile\" \"cd client && npm run build\"",
    "start": "NODE_OPTIONS=--force-node-api-uncaught-exceptions-policy=true ts-node server/index.ts",
    "compile": "tsc && cmake-js --out=build -d ../ --CDCHIAKI_ENABLE_STEAMDECK_NATIVE=OFF --CDCHIAKI_ENABLE_CLI=OFF --CDCHIAKI_ENABLE_GUI=OFF compile",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/node-persist": "^3.1.8",
    "@types/socket.io": "^3.0.2",
    "bindings": "^1.5.0",
    "cmake-js": "^7.3.0",
    "express": "^4.18.2",
    "node-addon-api": "^7.1.0",
    "node-persist": "^4.0.1",
    "npm": "^10.2.5",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.3"
  }
}
